    "keywords": [
        "shawnsandy",
        "Site Pages"
    ],
    "homepage": "https://github.com/devbox12/SitePages",
    "license": "MIT",
    "authors": [
        {
            "name": "Shawn Sandy",
            "email": "shawnsandy04@gmail.com",
            "homepage": "https://github.com/devbox12/SitePages",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.6"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "shawnsandy\\pages\\": "src"
        }
